{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "indent": ["error", 2],
    "linebreak-style": 0
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "sourceType": "module",
    "requireConfigFile": false,
    "allowImportExportEverywhere": true,
    "ecmaVersion": 2018
  }

}
